# Production Environment Configuration
# DBSpend360 - Databricks Cost Analytics Dashboard

[cloud]
# Production environment using GCP
platform = GCP

[databricks]
# SQL Warehouse Configuration for production
warehouse_id = dbdemos-shared-endpoint
# Data Source Configuration
table_name = production.dbcost360.databricks_job_spends
# Optional: Schema for organizing data
schema_name = production.dbcost360

[features]
# Feature flags for production
enable_cost_analysis = true
enable_cluster_analysis = true
enable_ai_insights = true
enable_export = true

[performance]
# Performance tuning for production
query_timeout_seconds = 60
cache_ttl_minutes = 15
max_results_per_page = 200

[logging]
# Logging configuration
log_level = WARN
enable_debug_endpoints = false