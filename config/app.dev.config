# Development Environment Configuration
# DBSpend360 - Databricks Cost Analytics Dashboard

[cloud]
# Supported platforms: AWS, Azure, GCP
platform = Azure

[databricks]
# SQL Warehouse Configuration
warehouse_id = 148ccb90800933a1
# Data Source Configuration (Full qualified table name)
table_name = dbspend360.test1.dbspend360_total_job_spends
# Optional: Schema for organizing data (Full qualified schema name)
schema_name = dbspend360.test1

[features]
# Feature flags for development
enable_cost_analysis = true
enable_cluster_analysis = true
enable_ai_insights = false
enable_export = true

[performance]
# Performance tuning for development
query_timeout_seconds = 30
cache_ttl_minutes = 5
max_results_per_page = 100

[logging]
# Logging configuration
log_level = INFO
enable_debug_endpoints = true