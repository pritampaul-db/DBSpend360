# Test Environment Configuration
# DBSpend360 - Databricks Cost Analytics Dashboard

[cloud]
# Test environment using Azure
platform = Azure

[databricks]
# SQL Warehouse Configuration for test environment
warehouse_id = test-warehouse-azure-001
# Data Source Configuration
table_name = test_env.dbcost360.databricks_job_spends
# Optional: Schema for organizing data
schema_name = test_env.dbcost360

[features]
# Feature flags for testing
enable_cost_analysis = true
enable_cluster_analysis = true
enable_ai_insights = false
enable_export = false

[performance]
# Performance tuning for test environment
query_timeout_seconds = 45
cache_ttl_minutes = 10
max_results_per_page = 50

[logging]
# Logging configuration
log_level = DEBUG
enable_debug_endpoints = true